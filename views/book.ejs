<body>
  <nav>
    <button class="voltar" onclick="history.back()"><i class="fa-solid fa-arrow-left"></i></button>
    <h1>EBJN</h1>
  </nav>
  <div id="flipbook">
    <% for(let i = 1; i < query[0].pages; i++) { %>
      <div>
        <img class="page" src="/PAGES/<%= placeholder%>/<%= placeholder + i%>.png">
      </div>
    <% }%>
  </div>
  <script src="https://code.jquery.com/jquery-3.7.1.js"></script>
        <link rel="stylesheet" href="/css/nprogress">
        <script src="/js/nprogress"></script>
        <script>
            $('body').show();
            $('.version').text(NProgress.version);
            NProgress.start();
            var interval = setInterval(() => NProgress.inc(), 1000);
        </script>
        <script>
          $(window).on('load', () => {
            clearInterval(interval);
            NProgress.done();
          });
        </script>
</body>
